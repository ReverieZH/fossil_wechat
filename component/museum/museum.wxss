fossillist-head-img{
  width: 100%;
  height: 180rpx;
  padding: 20rpx;
  box-sizing: border-box;
}
.fossillist{
  margin-top: 20rpx;
}
.fossil-list-info{
  float: left;
  margin-left: 16rpx;
  margin-top: 20rpx;
  position: relative;
  border: solid rgb(241, 241, 241) 1rpx;
}

.fossil-list-photo{
  width: 350rpx;
  height: 250rpx;
}
.fossil-list-desc{
  display: block;
  color: #000;
  bottom: 3%;
  left: 0;
  width: 94%;
  text-align: center;
  font-size: 25rpx;
  padding: 10rpx;
  opacity: 0.7;
}